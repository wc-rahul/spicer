<ul class="menu_logo_list" data-mobile-menu="link-{{ block.settings.target | handleize }}">
  {% for entry in block.settings.metaobject %}
    <li class="menu_logo_item">
      <a href="{{ entry.link }}" class="logo_img_link">
        {{ entry.logo | image_url: width: '300', height: '100' | image_tag: class: 'menu_link_logo_img' }}
      </a>
    </li>
  {% endfor %}
</ul>
{% schema %}
{
  "name": "Visual Menu",
  "tag": null,
  "settings": [
    {
      "type": "metaobject_list",
      "id": "metaobject",
      "label": "Select Metaobject",
      "metaobject_type": "photo_fine_art_menu"
    },
    {
      "type": "header",
      "content": "Mobile Drawer Only"
    },
    {
      "type": "text",
      "id": "target",
      "label": "Enter Target Link",
      "info": "Enter Print to display content in print menu"
    }
  ],
  "blocks": [],
  "presets": [
    {
      "name": "Visual Links"
    }
  ]
}
{% endschema %}
